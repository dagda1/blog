class BusinessUnitsView extends MIBaseView
  events:
    'click #generate': "generate"

  generate: (e) =>    
    e.preventDefault()

    identifiers = _.map( $('input:checked'), (check) -> 
                    {Uid: $(check).val(), Title: $(check).parents("tr:eq(0)").find("label").html()}
                  )

    @managementReportView = new ManagementReportView({collect:  new BusinessUnits(identifiers)})

    @managementReportView.render()