=form_for @user_session, :id => 'login', :url => user_session_path do |f|
  =f.error_messages
  %h2 
    Log In
  %fieldset#inputs
    =f.label :email
    =f.text_field(:email, {:placeholder => "email", :autofocus => "autofocus", :required => "required", :autocomplete => "off"})
    =f.label :password
    =f.password_field(:password, {:placeholder => "password"})
  %fieldset#actions
    =f.submit("Log In")