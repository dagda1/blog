<table id="results">
  <thead>
    <tr>
      <th class="first">&nbsp;</th>
      <th>&nbsp;</th>
      <th>Company Name</th>
      <th>Web</th>
      <th>Phone 1</th>
      <th>Phone 2</th>
      <th>Fax</th>
    </tr>
  </thead>
  {{#each Lead.leads_controller}}
    {{#view Lead.Views.Leads contentBinding="this"}}
    <tr>
      <td>
        <strong>{{adjustedIndex}}</strong>
      </td>
      <td class="plus expander">&nbsp;</td>
      <td>{{content.company}}</td>
      <td>{{content.web}}</td>
      <td>{{content.phone_1}}</td>
      <td>{{content.phone_2}}</td>
      <td>{{content.fax}}</td>
    </tr>
    {{/view}}
  {{/each}}
</table>
