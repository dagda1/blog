            {{#if content.hasEmails}}
              {{#each content.emails}}
                {{#view Lead.Views.Leads contentBinding="this"}}
                  <tr>
                    <td class="first"><strong>{{adjustedIndex}}</strong>
                    <td>{{content.email}}</td>
                  </tr>
                {{/view}}
              {{/each}}
            {{else}}
              <tr>
                <td colspan="2">No email addresses have been found</td>
              <tr>
            {{/if}}