Lead.Controllers.Leads = Ember.ArrayProxy.extend
  init: ->
    @_super()

    @set 'content', Ember.A()

    @view = Ember.View.create
      templateName: 'app/templates/leads/list'

    @view.appendTo $('#leads')

  addLead: (data) ->
    lead = Lead.Lead.create()
    lead.setProperties JSON.parse data
    @pushObject lead